package FirstPackage;

public class FinalKeyword {
	
	// Final Keyword- We can't change the values of the variables which are declared as Final

	public static void main(String[] args) {
		
		
		final int x = 10;
		
		System.out.println(x);
		
		x = 50;
		
		System.out.println(x);
		
		x = 70;
		
		System.out.println(x);

	}

}
