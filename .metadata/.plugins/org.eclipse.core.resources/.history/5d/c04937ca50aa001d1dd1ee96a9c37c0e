package FirstPackage;

public class ClassAccessModifier1 {
	
	//public - variables and Methods that are declared as public can be accessed by any class from same package or other package also
	//default - variables and Methods that are declared as default can be accessed by any class from the same package. classes from other package cannot access.
	//
	public
	protected
	private
	default

	public static void main(String[] args) {
		

	}

}
