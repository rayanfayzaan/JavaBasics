package FirstPackage;

public class ThisKeyword {
	
	int y = 10; // instance or Global Variable
	
	public void getData()
	{
		//int y = 20; // local variable
		System.out.println(y);
	}
	
	public void login()
	{
		System.out.println(y);
	}

	public static void main(String[] args) {

		ThisKeyword S = new ThisKeyword();
		S.getData();
		S.login();
		

	}

}
