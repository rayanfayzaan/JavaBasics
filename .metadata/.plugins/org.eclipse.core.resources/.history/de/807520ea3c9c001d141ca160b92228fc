package FirstPackage;

public class SampleTest {
	
	public static String[] getData()
	{
		String name = "Anees";
		String sex = "Male";
		String[] details = new String[2];
		details[0]=name;
		details[1]=sex;
		return details;
	}

	public static void main(String[] args) {
		
          String firstname =getData();
          System.out.println(firstname);
	}

}
