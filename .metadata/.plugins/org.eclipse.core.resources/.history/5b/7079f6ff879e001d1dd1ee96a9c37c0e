		String name ="Anees";
		
		if(name.equals("Qasim"))
		{
			System.out.println("I will talk to Qasim");
		}
		else if(name.equals("Usman"))
		{
			System.out.println("I will talk to Usman");
		}
		else if(name.equals("Achala"))
