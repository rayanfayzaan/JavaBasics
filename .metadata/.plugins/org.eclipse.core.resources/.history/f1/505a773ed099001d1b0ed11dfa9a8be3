package FirstPackage;

public class MyFirstProgram {

	public static void main(String[] args) {
		
		//type SYSO and press Ctrl+spacebar - it is a comment
		/*
		System.out.println("Hello");
		System.out.println("Team");
		System.out.println("how are you");
		System.out.println("Hello");
		System.out.println("Team");
		*/
		System.out.println("how are you");
		
		//Variables - variables are containers for storing data values
		
		/*DataTypes - There are two types
		1. Primitive DataType
		2. non-Primitive DataType
		*/
		
		// int - we can store only whole numbers 100; we cant save decimal number in int
		// double - we can store a value with decimal point - 100.50
		// char - we can store only single character - A,B,C,D,E
		// Boolean - True or False
		 int x= 250;
		 System.out.println(x);
		 
		 double y=100.85;
		 System.out.println(y);
	}

}
